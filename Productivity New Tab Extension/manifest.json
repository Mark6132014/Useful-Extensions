{
    "name": "Productivity New Tab Extension",
    "version": "1.0",
    "description": "It has everything you need for your new tab.",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "48": "icon.png"
    },
    "permissions": [
      "cookies",
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "contextMenus"
    ],
    "host_permissions": [
      "https://itzgametime-vip.w3spaces.com/"
    ],
    "web_accessible_resources": [
    {
      "resources": [
      "newtab.js",
    "newtab.html",
    "icon.png"],
      "matches": ["<all_urls>"]
    }
  ]
  }