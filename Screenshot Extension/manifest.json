{
    "name": "Screenshot Extension",
    "version": "1.0",
    "description": "Screenshot websites",
    "manifest_version": 3,
    "action": {
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "main.js"
        ]
      }
    ],
    "background": {
      "service_worker": "service.js"
    },
    "permissions": [
      "cookies",
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "contextMenus"
    ],
    "host_permissions": [
      "https://itzgametime-vip.w3spaces.com/",
      "https://youtube.com"
    ]
  }